{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "outDir": "lib_test",
    "lib": ["es6", "dom"],
    "target": "es6",
    "types": ["reflect-metadata"],
    "declaration": false,
    "removeComments": true,
    "inlineSourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
